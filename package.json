{
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "node ./scripts/check-port.js && npm run dev:watch",
    "dev:watch": "concurrently \"npm:dev:watch-*\" -k",
    "dev:watch-tsc": "tsc --build --clean && tsc -w",
    "dev:watch-electron": "wait-on file:./dist/App.js && cross-env NODE_ENV=development electronmon ./dist/App.js",
    "dev:watch-parcel": "cross-env PORT=1234 parcel serve ./src/render/Index.html --dist-dir ./dist/render --no-cache",
    "test": "cross-env NODE_ENV=test nyc mocha -r ts-node/register ./src/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.27.2",
    "chai": "^4.3.6",
    "concurrently": "^7.2.2",
    "detect-port": "^1.3.0",
    "electron-mocha": "^11.0.2",
    "electronmon": "^2.0.2",
    "nyc": "^15.1.0",
    "p-limit": "3.1.0",
    "prettier": "2.7.1",
    "process": "^0.11.10",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.3",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@types/detect-port": "^1.3.2",
    "@types/yargs": "^17.0.10",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.4",
    "mocha": "^10.0.0",
    "parcel": "^2.6.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "ts-node": "^10.8.1",
    "yargs": "^17.5.1"
  }
}
